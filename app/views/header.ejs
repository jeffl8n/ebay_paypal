<!doctype html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width">
  <title><%-title %></title>
  <link rel="stylesheet" href="/css/style.css">

  <script src="/js/angular.min.js"></script>

  <script src="/js/d3.min.js"></script>
  <script src="/js/nv.d3.min.js"></script>
  <script src="/js/angular-nvd3.min.js"></script>
  <script src="/js/angularjs-nvd3-directives.min.js"></script>
  <link rel="stylesheet" href="/css/nv.d3.min.css">

  <script src="/js/jquery.min.js"></script>

  <!-- load bootstrap  -->
  <link rel="stylesheet" href="/css/bootstrap.min.css">
  <link rel="stylesheet" href="/css/bootstrap-theme.min.css">
  <script src="/js/bootstrap.min.js"></script>

  <style>
    html                    { overflow-y:scroll; }
    body                    { padding-top:50px; }
    .top-form             { margin-bottom:30px; }
     <% for (pair in paypalColors){ %>
.paypal .<%=paypalColors[pair].catagory%>{
  color: <%=paypalColors[pair].color%>;
}
.paypal .<%=paypalColors[pair].catagory%>-bg{
  background-color: <%=paypalColors[pair].color%>;
}
    <%}%>
  <% for (pair in ebayColors){ %>
.ebay .<%=ebayColors[pair].catagory%>{
  color: <%=ebayColors[pair].color%>;
}
.ebay .<%=ebayColors[pair].catagory%>-bg{
  background-color: <%=ebayColors[pair].color%>;
}
    <%}%>
   
</style>
<script>
var paypalColors = []
<% for (pair in paypalColors){ %>
paypalColors.push({catagory:'<%=paypalColors[pair].catagory%>',color: '<%=paypalColors[pair].color%>'});
    <%}%>

  var ebayColors = []
<% for (pair in ebayColors){ %>
ebayColors.push({catagory:'<%=ebayColors[pair].catagory%>',color: '<%=ebayColors[pair].color%>'});
    <%}%>
</script>

  <% if (ENV_DEVELOPMENT) { %>
    <script src="http://localhost:35729/livereload.js"></script>
  <% } %>
</head>
